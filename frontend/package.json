{
	"name": "cloud99-frontend",
	"version": "1.0.0",
	"description": "The frontend for cloud99.",
	"main": "index.js",
	"scripts": {
		"build:dev": "webpack -d --config webpack.dev.js",
		"build:prod": "webpack --config webpack.prod.js --mode production",
		"lint": "eslint .; exit 0",
		"lint:fix": "eslint . --fix",
		"serve:dev": "webpack-dev-server --config webpack.dev.js --mode development --module-bind js=babel-loader --inline --hot --host 0.0.0.0",
		"start": "webpack-dev-server --config webpack.dev.js --mode development --open --host 0.0.0.0",
		"watch": "webpack-dev-server --config webpack.dev.js --inline --hot"
	},
	"keywords": [
		"cloud99",
		"digitalocean",
		"droplet"
	],
	"author": "Bharath Kumar Ravichandran",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "https://github.com/BharathKumarRavichandran/cloud99/frontend"
	},
	"dependencies": {
		"axios": "0.19.0",
		"compression-webpack-plugin": "^3.0.0",
		"dotenv": "^8.1.0",
		"dotenv-webpack": "^1.7.0",
		"express": "^4.17.1",
		"fs": "0.0.1-security",
		"path": "^0.12.7",
		"react-router": "5.0.1",
		"react-router-dom": "5.0.1",
		"uglifyjs-webpack-plugin": "^2.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.4.5",
		"@babel/preset-env": "^7.4.5",
		"@babel/preset-react": "^7.0.0",
		"babel-eslint": "^10.0.1",
		"babel-loader": "^8.0.6",
		"css-loader": "^2.1.1",
		"eslint": "^5.16.0",
		"eslint-loader": "^2.1.2",
		"eslint-plugin-react": "^7.13.0",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.2.0",
		"mini-css-extract-plugin": "^0.7.0",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"webpack": "^4.33.0",
		"webpack-cli": "^3.3.4",
		"webpack-dev-server": "^3.7.1"
	}
}
